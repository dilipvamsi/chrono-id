{
    "valid_cases": [
        {
            "comment": "Unix Epoch",
            "iso": "1970-01-01T00:00:00Z",
            "variants": [
                {
                    "name": "chrono64",
                    "expected_timestamp": 0
                },
                {
                    "name": "uchrono64",
                    "expected_timestamp": 0
                },
                {
                    "name": "chrono64ms",
                    "expected_timestamp": 0
                },
                {
                    "name": "uchrono64ms",
                    "expected_timestamp": 0
                }
            ]
        },
        {
            "comment": "Epoch 2000",
            "iso": "2000-01-01T00:00:00Z",
            "variants": [
                {
                    "name": "chrono32",
                    "expected_timestamp": 0
                },
                {
                    "name": "uchrono32",
                    "expected_timestamp": 0
                },
                {
                    "name": "chrono32h",
                    "expected_timestamp": 0
                },
                {
                    "name": "uchrono32h",
                    "expected_timestamp": 0
                },
                {
                    "name": "chrono32m",
                    "expected_timestamp": 0
                },
                {
                    "name": "uchrono32m",
                    "expected_timestamp": 0
                },
                {
                    "name": "chrono32w",
                    "expected_timestamp": 0
                },
                {
                    "name": "uchrono32w",
                    "expected_timestamp": 0
                }
            ]
        },
        {
            "comment": "Standard date with ms",
            "iso": "2023-05-20T10:30:00.123Z",
            "variants": [
                {
                    "name": "chrono64ms",
                    "expected_iso": "2023-05-20T10:30:00.123Z"
                }
            ]
        },
        {
            "comment": "Standard date with us",
            "iso": "2023-05-20T10:30:00.123456Z",
            "variants": [
                {
                    "name": "chrono64us",
                    "expected_iso": "2023-05-20T10:30:00.123456Z"
                }
            ]
        }
    ],
    "error_cases": [
        {
            "name": "null_input",
            "input": null,
            "expected_error": "Input string is null"
        },
        {
            "name": "invalid_format",
            "input": "not-a-date",
            "expected_error": "Invalid ISO 8601 format"
        },
        {
            "name": "underflow_1970",
            "input": "1960-01-01T00:00:00Z",
            "variant": "chrono64",
            "expected_error": "Timestamp underflow: Date is before Unix Epoch (1970-01-01)"
        },
        {
            "name": "underflow_2000",
            "input": "1999-12-31T23:59:59Z",
            "variant": "chrono32",
            "expected_error": "Timestamp underflow: Date is before Epoch (32-bit types require 2000-01-01 or later)"
        }
    ]
}